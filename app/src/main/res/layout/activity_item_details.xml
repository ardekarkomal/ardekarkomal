<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android">
<LinearLayout
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <include
        android:id="@+id/toolbar"
        layout="@layout/dashbord_toolbar"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/details"
        android:textSize="@dimen/_16sdp"
        android:textColor="@color/black"
        android:paddingTop="@dimen/_20sdp"
        android:paddingStart="@dimen/_12sdp"
        android:paddingBottom="@dimen/_12sdp"
        android:paddingLeft="@dimen/_12sdp"
        tools:ignore="RtlSymmetry" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="@dimen/_10sdp"
        android:paddingStart="@dimen/_12sdp"
        android:paddingLeft="@dimen/_12sdp"
        tools:ignore="RtlSymmetry">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/order_id"
        android:textColor="@color/black"
        android:textSize="@dimen/_10sdp"
        android:textStyle="bold"/>
    <TextView
        android:id="@+id/tv_orderId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:text="order_l00uiedsdjshdjs"
        android:paddingStart="@dimen/_44sdp"
        android:paddingLeft="@dimen/_44sdp"
        android:textSize="@dimen/_10sdp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="@dimen/_8sdp"
        android:paddingStart="@dimen/_12sdp"
        android:paddingLeft="@dimen/_12sdp"
        tools:ignore="RtlSymmetry">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/createdate"
        android:textSize="@dimen/_10sdp"
        android:textColor="@color/black"
        android:textStyle="bold"/>
    <TextView
        android:id="@+id/tv_createDate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:text="21-09-2021 11:09:26"
        android:paddingStart="@dimen/_30sdp"
        android:paddingLeft="@dimen/_30sdp" />
    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="@dimen/_8sdp"
        android:paddingStart="@dimen/_12sdp"
        android:paddingLeft="@dimen/_12sdp"
        tools:ignore="RtlSymmetry">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/total"
            android:textSize="@dimen/_10sdp"
            android:textColor="@color/black"
            android:textStyle="bold"/>
        <TextView
            android:id="@+id/tv_total"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:text="1050.00"
            android:paddingStart="@dimen/_55sdp"
            android:paddingLeft="@dimen/_55sdp" />
    </LinearLayout>

    <HorizontalScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingEnd="@dimen/_12sdp"
        android:paddingRight="@dimen/_12sdp"
        android:overScrollMode="never"
        tools:ignore="RtlSymmetry">
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical">
    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/_1sdp"
        android:background="@color/divider_color"
        android:layout_marginTop="@dimen/_10sdp"/>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingLeft="@dimen/_12sdp"
        android:paddingTop="@dimen/_12sdp"
        android:paddingBottom="@dimen/_12sdp"
        android:paddingRight="0dp"
        android:paddingEnd="0dp"
        android:paddingStart="@dimen/_12sdp">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/item_details"
            android:textSize="@dimen/_10sdp"
            android:textStyle="bold"
            android:textColor="@color/black"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/description"
            android:textSize="@dimen/_10sdp"
            android:textColor="@color/black"
            android:textStyle="bold"
            android:paddingStart="@dimen/_90sdp"
            android:paddingLeft="@dimen/_90sdp"
            tools:ignore="RtlSymmetry" />
    </LinearLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:paddingTop="@dimen/_12sdp"
            tools:ignore="RelativeOverlap,RtlSymmetry">
        <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/unitprice"
                android:textColor="@color/black"
                android:textStyle="bold"
                android:textSize="@dimen/_10sdp"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/quntity"
                android:textColor="@color/black"
                android:textStyle="bold"
                android:textSize="@dimen/_10sdp"
                android:paddingStart="@dimen/_15sdp"
                android:paddingLeft="@dimen/_15sdp"
                tools:ignore="RtlSymmetry" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/total_without_colon"
                android:textColor="@color/black"
                android:textStyle="bold"
                android:textSize="@dimen/_10sdp"
                android:paddingStart="@dimen/_15sdp"
                android:paddingLeft="@dimen/_15sdp"
                tools:ignore="RtlSymmetry" />
        </LinearLayout>
    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/_1sdp"
        android:background="@color/divider_color" />
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingTop="@dimen/_10sdp"
        android:paddingStart="@dimen/_12sdp"
        android:paddingLeft="@dimen/_12sdp"
        tools:ignore="RtlSymmetry">

        <ImageView
            android:id="@+id/iv_item_image"
            android:layout_width="@dimen/_55sdp"
            android:layout_height="@dimen/_40sdp"
            android:contentDescription="@string/img_desc"
            android:scaleType="fitXY"
            android:src="@drawable/insurance2" />
        <TextView
            android:id="@+id/tv_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:text="Life-individual"
            android:textSize="@dimen/_8sdp"
            android:layout_toEndOf="@id/iv_item_image"
            android:layout_toRightOf="@id/iv_item_image"
            android:paddingStart="@dimen/_22sdp"
            android:paddingLeft="@dimen/_22sdp"
            tools:ignore="RtlSymmetry" />
        <TextView
            android:id="@+id/tv_desc"
            android:layout_width="@dimen/_80sdp"
            android:layout_height="wrap_content"
            tools:text="Life-individual sjjhshdjshjshd"
            android:textSize="@dimen/_8sdp"
            android:layout_toEndOf="@id/tv_title"
            android:layout_toRightOf="@id/tv_title"
            android:paddingStart="@dimen/_20sdp"
            android:paddingLeft="@dimen/_20sdp"
            android:singleLine="false"
            tools:ignore="RtlSymmetry"/>
        <TextView
            android:id="@+id/tv_unit_price"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:text="1000"
            android:textSize="@dimen/_8sdp"
            android:layout_toEndOf="@id/tv_desc"
            android:layout_toRightOf="@id/tv_desc"
            android:paddingStart="@dimen/_10sdp"
            android:paddingLeft="@dimen/_10sdp"
            android:layout_centerHorizontal="true"
            tools:ignore="RtlSymmetry" />

        <TextView
            android:id="@+id/tv_quantity"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:text="1"
            android:textSize="@dimen/_8sdp"
            android:layout_toEndOf="@id/tv_unit_price"
            android:layout_toRightOf="@id/tv_unit_price"
            android:paddingStart="@dimen/_35sdp"
            android:paddingLeft="@dimen/_35sdp"
            tools:ignore="RtlSymmetry" />

        <TextView
            android:id="@+id/tv_total_amount"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:text="1000"
            android:textSize="@dimen/_8sdp"
            android:layout_toEndOf="@id/tv_quantity"
            android:layout_toRightOf="@id/tv_quantity"
            android:paddingStart="@dimen/_40sdp"
            android:paddingLeft="@dimen/_40sdp"           tools:ignore="RtlSymmetry" />
    </RelativeLayout>
    </LinearLayout>
    </HorizontalScrollView>
</LinearLayout>
</layout>
